<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hazelnut.dummy">

    <application
        android:allowBackup="true"
        android:icon="@drawable/innerchain"
        android:label="Innerchain"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".BlacklistActivity"></activity>
        <activity android:name=".ViewActivity"></activity>
        <activity android:name=".UpdateActivity"></activity>
        <activity android:name=".MainActivity" android:label="Dummy Main"></activity>
        <activity android:name=".BranchActivity" android:label="Dummy Main"></activity>
        <activity android:name=".RegisterActivity" android:label="Register Page"></activity>
        <activity android:name=".Register2Activity"></activity>
        <activity android:name=".Register3Activity"></activity>
        <activity android:name=".Register4Activity"></activity>
        <activity android:name=".DevActivity"></activity>
        <activity android:name=".LoginDevActivity"></activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-feature android:name="android.hardware.camera2.full" />
</manifest>